{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/六爻",
      "dest": "/liuyao-calculator.html"
    },
    {
      "src": "/八字",
      "dest": "/enhanced-bazi-calculator.html"
    },
    {
      "src": "/mobile",
      "dest": "/mobile-bazi-calculator.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "functions": {},
  "rewrites": []
}